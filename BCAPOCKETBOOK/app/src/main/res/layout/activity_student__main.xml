                                  android:gravity="center_horizontal"
                                  android:text="Home Work"
                                  android:textAlignment="center"
                                  android:textColor="#000000"
                                  android:textSize="22sp" />

                              <ImageView
                                  android:id="@+id/homework"
                                  android:layout_width="89dp"
                                  android:layout_height="85dp"
                                  android:layout_alignParentStart="true"
                                  android:layout_alignParentLeft="true"
                                  android:layout_alignParentTop="true"
                                  android:layout_alignParentEnd="true"
                                  android:layout_alignParentRight="true"
                                  android:layout_marginStart="48dp"
                                  android:layout_marginLeft="48dp"
                                  android:layout_marginTop="18dp"
                                  android:layout_marginEnd="38dp"
                                  android:layout_marginRight="38dp"
                                  android:gravity="center_horizontal"
                                  android:scaleType="fitCenter"
                                  android:textAlignment="center"
                                  app:srcCompat="@drawable/contract"
                                  tools:ignore="VectorDrawableCompat" />

                          </RelativeLayout>

                      </androidx.cardview.widget.CardView>

                  </LinearLayout>

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="180dp"
                      android:id="@+id/linearLayout2"
                      android:layout_marginTop="5dp"
                      android:orientation="horizontal"
                      app:layout_constraintEnd_toEndOf="parent"
                      app:layout_constraintStart_toStartOf="parent"
                      app:layout_constraintTop_toBottomOf="@+id/linearLayout1"
                      tools:ignore="MissingConstraints">

                      <androidx.cardview.widget.CardView
                          android:layout_width="185dp"
                          android:layout_height="match_parent"
                          android:layout_margin="10dp"
                          android:padding="5dp"
                          android:id="@+id/cardOldPaper"
                          app:cardCornerRadius="10dp"
                          android:foreground="?android:attr/selectableItemBackground"
                          app:cardElevation="18dp">

                          <RelativeLayout
                              android:layout_width="match_parent"
                              android:layout_height="match_parent"
                              android:background="#360034CD">

                              <TextView
                                  android:id="@+id/tvOldpaper"
                                  android:layout_width="127dp"
                                  android:layout_height="47dp"
                                  android:layout_below="@+id/oldpaper"
                                  android:layout_alignParentStart="true"
                                  android:layout_alignParentLeft="true"
                                  android:layout_alignParentEnd="true"
                                  android:layout_alignParentRight="true"
                                  android:layout_marginStart="32dp"
                                  android:layout_marginLeft="32dp"
                                  android:layout_marginTop="7dp"
                                  android:layout_marginEnd="26dp"
                                  android:layout_marginRight="26dp"
                                  android:fontFamily="@font/amaranth"
                                  android:gravity="center_horizontal"
                                  android:text="Old Paper"
                                  android:textAlignment="center"
                                  android:textColor="#000000"
                                  android:textSize="24sp" />

                              <ImageView
                                  android:id="@+id/oldpaper"
                                  android:layout_width="130dp"
                                  android:layout_height="91dp"
